{
  "name": "sick-fits-frontend",
  "version": "1.0.2",
  "description": "This is Indie Tea's Frontend. Built with React, GraphQL, Next and Apollo",
  "main": "index.js",
  "author": "Jimmy Zhang",
  "license": "ISC",
  "dependencies": {
    "@material-ui/core": "latest",
    "@material-ui/icons": "latest",
    "@material-ui/styles": "latest",
    "@zeit/next-css": "^1.0.1",
    "apollo-boost": "^0.1.16",
    "apollo-client": "^2.4.2",
    "async": "^3.1.0",
    "autoprefixer": "^9.6.1",
    "aws-amplify": "^1.1.36",
    "aws-amplify-react-jz": "^2.3.14",
    "axios": "^0.19.0",
    "babel-core": "^7.0.0-bridge.0",
    "clsx": "latest",
    "date-fns": "^2.0.0-alpha.7",
    "dotenv": "6.0.0",
    "dotenv-load": "^2.0.0",
    "downshift": "^2.2.3",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.9.2",
    "lodash.debounce": "^4.0.8",
    "next": "^11.1.0",
    "next-compose-plugins": "^2.2.0",
    "next-env": "^1.1.0",
    "next-server": "^9.0.5",
    "next-with-apollo": "^3.1.3",
    "nprogress": "^0.2.0",
    "path-match": "^1.2.4",
    "postcss-loader": "^3.0.0",
    "prop-types": "latest",
    "react": "latest",
    "react-adopt": "^0.6.0",
    "react-apollo": "^2.2.1",
    "react-dom": "latest",
    "react-stripe-checkout": "^2.6.3",
    "react-transition-group": "^2.5.0",
    "serverless-apigw-binary": "^0.4.4",
    "serverless-domain-manager": "^3.3.0",
    "serverless-http": "^2.3.0",
    "style-loader": "^1.0.0",
    "url": "^0.11.0",
    "url-loader": "^2.0.1",
    "webpack": "^4.39.2"
  },
  "devDependencies": {
    "babel-jest": "^23.6.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "casual": "^1.5.19",
    "css-loader": "1.0.1",
    "enzyme-to-json": "^3.3.4",
    "graphql-tools": "^4.0.0",
    "jest": "^23.6.0",
    "jest-transform-graphql": "^2.1.0",
    "serverless-nextjs-plugin": "^2.4.0"
  },
  "scripts": {
    "dev": "NODE_ENV=development next -p 7777",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "stage": "serverless deploy -v --stage staging",
    "production": "serverless deploy -v --stage production",
    "test": "NODE_ENV=test jest --watch"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      ".*": "babel-jest"
    }
  },
  "//": "This is our babel config, I prefer this over a .babelrc file",
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": []
      },
      "production": {
        "presets": [
          "next/babel"
        ],
        "plugins": []
      },
      "test": {
        "presets": [
          [
            "next/babel",
            {
              "preset-env": {
                "modules": "commonjs"
              }
            }
          ]
        ]
      }
    }
  }
}
